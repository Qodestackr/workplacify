{
  "name": "workplaciyf",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "generate": "prisma generate",
    "prisma-studio": "prisma studio",
    "db-up": "docker-compose up -d",
    "db-seed": "prisma db seed",
    "db-reset": "prisma migrate dev reset",
    "dx:next": "run-s db-up migrate-dev db-seed && next dev --experimental-https",
    "dx": "run-p dx:* --print-label",
    "dev": "npm run dx:next",
    "prebuild": "run-s generate migrate",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --cache --ext \".js,.ts,.tsx\" --report-unused-disable-directives --report-unused-disable-directives src",
    "lint-fix": "npm run lint --fix",
    "migrate-dev": "prisma migrate dev",
    "prisma-case-format": "cross-env prisma-case-format --file ./prisma/schema.prisma --map-table-case=snake --map-field-case=snake --map-enum-case=snake",
    "migrate": "prisma migrate deploy",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "test": "run-s test:*",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "test-dev": "start-server-and-test dev http://127.0.0.1:3000 test",
    "test-start": "start-server-and-test start http://127.0.0.1:3000 test",
    "postinstall": "npm run generate"
  },
  "engines": {
    "node": ">=20.8.1"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.8",
    "@chakra-ui/next-js": "^2.2.0",
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@prisma/client": "^5.6.0",
    "@tanstack/react-query": "^4.36.1",
    "@trpc/client": "^10.44.0",
    "@trpc/next": "^10.44.0",
    "@trpc/react-query": "^10.44.0",
    "@trpc/server": "^10.44.0",
    "clsx": "^2.0.0",
    "framer-motion": "^10.16.5",
    "next": "^14.0.3",
    "next-auth": "^4.24.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "superjson": "^2.2.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^20.9.4",
    "@types/react": "^18.2.38",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "eslint": "^8.54.0",
    "eslint-config-next": "^14.0.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "prisma": "^5.6.0",
    "prisma-case-format": "^2.0.1",
    "start-server-and-test": "^2.0.3",
    "tsx": "^4.2.0",
    "typescript": "^5.3.2",
    "vite": "^5.0.2",
    "vitest": "^0.34.6"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
